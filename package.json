{
  "name": "tsdav",
  "version": "1.1.1",
  "description": "WebDAV, CALDAV, and CARDDAV client for Nodejs and the Browser",
  "keywords": [
    "dav",
    "caldav",
    "carddav",
    "webdav",
    "ical",
    "vcard",
    "addressbook",
    "calendar",
    "calendars",
    "contacts",
    "contact",
    "sync",
    "nodejs",
    "browser",
    "typescript"
  ],
  "homepage": "https://tsdav.vercel.app/",
  "repository": "https://github.com/llldar/tsDAV",
  "license": "MIT",
  "author": "linlilulll@gmail.com",
  "main": "dist",
  "module": "dist-esm",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "dist-esm",
    "package.json"
  ],
  "scripts": {
    "build": "yarn -s clean && tsc -p tsconfig.cjs.json && tsc -p tsconfig.esm.json && copyfiles package.json LICENSE README.md ./dist && copyfiles package.json LICENSE README.md ./dist-esm",
    "clean": "rimraf dist*",
    "lint": "eslint src --ext .ts",
    "lintFix": "eslint src --ext .ts --fix",
    "prepublishOnly": "yarn build",
    "test": "concurrently \"yarn test:unit\" \"yarn test:integration\"",
    "test:apple": "jest --watchAll --testPathPattern=src/__tests__/integration/apple",
    "test:google": "jest --watchAll --testPathPattern=src/__tests__/integration/google",
    "test:integration": "concurrently \"yarn test:apple\" \"yarn test:google\"",
    "test:unit": "jest --watchAll --testPathPattern=src/__tests__/unit",
    "typecheck": "tsc --noEmit",
    "watch": "tsc --watch --outDir ./dist"
  },
  "dependencies": {
    "base-64": "1.0.0",
    "cross-fetch": "3.1.4",
    "debug": "4.3.2",
    "eslint-module-utils": "2.7.1",
    "xml-js": "1.6.11"
  },
  "devDependencies": {
    "@types/base-64": "1.0.0",
    "@types/debug": "4.1.7",
    "@types/jest": "27.0.2",
    "@types/node": "16.11.4",
    "@typescript-eslint/eslint-plugin": "5.1.0",
    "@typescript-eslint/parser": "5.1.0",
    "concurrently": "6.3.0",
    "copyfiles": "2.4.1",
    "cross-env": "7.0.3",
    "dotenv": "10.0.0",
    "eslint": "8.1.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-airbnb-typescript": "14.0.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "27.3.1",
    "prettier": "2.4.1",
    "rimraf": "3.0.2",
    "sort-package-json": "1.52.0",
    "ts-jest": "27.0.7",
    "typescript": "4.4.4"
  },
  "engines": {
    "node": ">=10"
  }
}