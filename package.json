{
  "name": "ts-dav",
  "version": "1.0.0",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "cross-env DEBUG=* ts-node-dev --respawn --transpile-only src/test.ts",
    "build": "rimraf dist && tsc --emitDeclarationOnly --outDir ./dist && babel src --out-dir dist --extensions .ts",
    "watch:ts": "tsc --watch --emitDeclarationOnly --outDir ./dist",
    "watch:babel": "babel src --out-dir dist --extensions .ts --watch",
    "watch": "concurrently \"yarn watch:ts\" \"yarn watch:babel\" "
  },
  "files":["dist", "package.json"],
  "dependencies": {
    "base-64": "^1.0.0",
    "cross-fetch": "^3.0.6",
    "debug": "^4.3.1",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-typescript": "^7.12.7",
    "@types/base-64": "^0.1.3",
    "@types/debug": "^4.1.5",
    "@types/node": "^14.14.9",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.14.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.2.0",
    "rimraf": "^3.0.2",
    "ts-node-dev": "^1.0.0",
    "typescript": "^4.1.2"
  }
}
